            integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" 
            crossorigin="anonymous"
        >
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        
    </head>
    <body>
        {% block body %}{% endblock %}
        
        <script src="https://code.jquery.com/jquery-3.7.1.js"></script>
